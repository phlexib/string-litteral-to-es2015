{
    "name": "string-litteral-to-es2015",
    "displayName": "String Litteral To ES2015",
    "description": "Transform a String Litteral into a ES2015 compatible string",
    "version": "0.0.1",
    "publisher": "benrohel",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.transformStringLitteral"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.transformStringLitteral",
