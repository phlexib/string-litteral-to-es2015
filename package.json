        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "babel-core": "^6.26.3",
        "babel-plugin-transform-es2015-template-literals": "^6.22.0",
        "eslint": "^4.11.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "dedent": "^0.7.0"
    }
